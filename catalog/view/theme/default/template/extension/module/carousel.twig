<div class="manuf onspro col-xs-12">
{#  <div class="head-t"> 
<h4 class="heading"><span>brands</span></h4>
</div>#}
<div class="container">
<div class="logosli col-xs-12">
  <div id="carousel{{ module }}" class="owl-carousel owl-theme carousel">
  {% for banner in banners %}
      <div class="multi-row">
    <div class="text-center col-xs-12 logosl">
      <div class="dinfos">
        {% if banner.link %}
        <a href="{{ banner.link }}"><img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive" /></a>
        {% else %}
        <img src="{{ banner.image }}" alt="{{ banner.title }}" class="img-responsive" />
      {% endif %}
    </div>
</div>
      </div> 
    {% endfor %}
        </div>
</div>
</div>
</div>
<script type="text/javascript">
    $(document).ready(function() {
    $("#carousel{{ module }}").owlCarousel({
    itemsCustom : [
    [0, 2],
    [480, 3],
    [576, 4],
    [768, 4],
    [992, 5],
    [1200, 6],
    [1600, 6]
    ],
      autoPlay: false,
      loop: true,
      navigationText: ['<i class="fa fa-angle-left" aria-hidden="true"></i>', '<i class="fa fa-angle-right" aria-hidden="true"></i>'],
      navigation : false,
      pagination:false
    });
    });
</script>